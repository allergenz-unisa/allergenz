{
  "name": "app-tirocinio",
  "version": "1.0.0",
  "description": "Material UI app ",
  "scripts": {
    "init:app": "node -r fs -e \"fs.rmSync('./app/node_modules', { recursive: true, force: true })\"",
    "install:app": "cd ./app && npm install",
    "install:all": "concurrently \"npm:install:app\"",
    "init:all": "concurrently \"npm:init:app\"",
    "start:app": "cd ./app && npm start",
    "start:api": "json-server --watch restaurant.db.json -p 3001",
    "start:all": "concurrently \"npm:start:api\" \"npm:start:app\""
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "concurrently": "^7.1.0",
    "json-server": "^0.17.0"
  }
}
